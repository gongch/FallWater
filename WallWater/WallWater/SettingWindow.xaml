<Window x:Class="WallWater.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WallWater"
        mc:Ignorable="d"
        Title="SettingWindow" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>

        <Button  Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="5" Padding="3" Click="Button_Click">确定</Button>
    </Grid>
</Window>
